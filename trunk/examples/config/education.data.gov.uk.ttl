#FORMATTERS

spec:HtmlFormatter
  a api:XsltFormatter ;
  api:name "html" ;
  api:mimeType "text/html" , "application/xhtml+xml" ;
  api:stylesheet "xslt/school.xsl" .

############################################################################################
#
# API DESCRIPTION
#
############################################################################################

spec:api
	a api:API ;
	rdfs:label "Edubase API"@en;
	api:maxPageSize "50";
	api:defaultPageSize "10" ;
	api:sparqlEndpoint <http://services.data.gov.uk/education/sparql> ;
	api:defaultViewer api:labelledDescribeViewer ;
  api:defaultFormatter spec:HtmlFormatter ;
	api:viewer 
    api:describeViewer ,
	  api:labelledDescribeViewer ;
	api:endpoint 
	  spec:scheme , spec:definition ,
	  spec:school ,
	  spec:schools , 
	  spec:schoolsPrimary , 
	  spec:schoolsSecondary , 
	  spec:schoolsByLocalAuthority ,
	  spec:schoolsByLocalAuthorityName ,
	  spec:schoolsByDistrict ,
	  spec:schoolsByDistrictName ,
	  spec:schoolsByWard ,
	  spec:schoolsByWardName ,
	  spec:schoolsByConstituency ,
	  spec:schoolsByConstituencyName ,
	  spec:schoolsByLatLong ,
	  spec:schoolCensus ,
	  spec:schoolCensuses
	.
    
#ENDPOINTS

## Definitions ##

spec:scheme
  a api:ItemEndpoint ;
  api:uriTemplate "/def/{scheme}" ;
  api:itemTemplate "http://education.data.gov.uk/def/{scheme}" ;
  api:defaultViewer spec:schemeViewer ;
  .

spec:schemeViewer
  a api:Viewer ;
  api:properties "prefLabel,altLabel,label,note,scopeNote,topConcept.prefLabel,comment" ;
  .

spec:definition
  a api:ItemEndpoint ;
  api:uriTemplate "/def/{scheme}/{item}" ;
  api:itemTemplate "http://education.data.gov.uk/def/{scheme}/{item}" ;
  api:defaultViewer spec:definitionViewer ;
  .

spec:definitionViewer
  a api:Viewer ;
  api:properties "type.label,prefLabel,altLabel,label,note,scopeNote,topConceptOf,broader.prefLabel,narrower.prefLabel,comment,range.label,domain.label" ;
  .

#Describe a school
spec:school
  a api:ItemEndpoint ;
  rdfs:label "School" ;
  api:uriTemplate "/doc/school/{number}" ;
  api:itemTemplate "http://education.data.gov.uk/id/school/{number}" ;
  api:defaultViewer spec:viewerMedium ;
  api:viewer
    spec:viewerProvision ,
    spec:viewerLocation ,
    spec:viewerPerformance ,
    spec:viewerAdmin .

#List all schools
spec:schools
	a api:ListEndpoint ;
	rdfs:label "Schools" ;
	api:uriTemplate "/doc/school" ;
	api:selector spec:schoolsSelector ;
	api:defaultViewer spec:viewerShort ;
	api:viewer
    spec:viewerShort ,
    spec:viewerMedium ,
    spec:viewerProvision ,
    spec:viewerLocation ,
    spec:viewerPerformance ,
    spec:viewerAdmin .	

#List all primary schools
spec:schoolsPrimary
	a api:ListEndpoint ;
	api:uriTemplate "/doc/school/phase/primary" ;
	api:selector [
	  api:parent spec:schoolsSelector ;
	  api:filter "phaseOfEducation.label=Primary"
	] ;
	api:defaultViewer spec:viewerShort ;
	api:viewer
    spec:viewerShort ,
    spec:viewerMedium ,
    spec:viewerProvision ,
    spec:viewerLocation ,
    spec:viewerPerformance ,
    spec:viewerAdmin .	

#List all secondary schools
spec:schoolsSecondary
	a api:ListEndpoint ;
	api:uriTemplate "/doc/school/phase/secondary" ;
	api:selector [
	  api:parent spec:schoolsSelector ;
	  api:filter "phaseOfEducation.label=Secondary"
	] ;
	api:defaultViewer spec:viewerShort ;
	api:viewer
    spec:viewerShort ,
    spec:viewerMedium ,
    spec:viewerProvision ,
    spec:viewerLocation ,
    spec:viewerPerformance ,
    spec:viewerAdmin .	

